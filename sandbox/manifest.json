{
    "name":"Rumi",
    "version":"0.1.0",
    "description":"Sleep on time, everytime.",
    "permissions":["storage","tabs"],
    "host_permissions":["https://*.youtube.com/*"],
    "background":{
        "service_worker":"background.js"
    },
    "content_scripts":[
        {
            "matches":["https://*.youtube.com/*"],
            "js":["contentScript.js"]
        }
    ],
    "web_accessible_resources":[
        {
            "resources":[
                "assets/rumi_sleeping.jpg"
            ],
            "matches":["https://*.youtube.com/*"]
        }
    ],
    "action":{
        "default_icon":{
            "16":"assets/rumi_sleeping.jpg",
            "24":"assets/rumi_sleeping.jpg",
            "32":"assets/rumi_sleeping.jpg"
        },
        "default_title":"Rumi",
        "default_popup":"popup.html"
    },
    "manifest_version":3
}